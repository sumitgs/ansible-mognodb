systemLog:
	destination: file
	path: "/var/log/mongodb/mongod.log"
	logAppend: true
storage:
	dbpath: "var/db/mongo"
	journal:
		enabled: true
processManagement:
	fork: true
	pidFilePath: "var/run/mongodb/mongod.pid"
net:
	http:
		enabled: true
		JSONPEnabled: true
		RESTInterfaceEnabled: true
	bindIp: 0.0.0.0